@article{phillipsbrittle2009,
  title = {Brittle Deformation Relating to the {{Carboniferous}}\textendash{{Cretaceous}} Evolution of the {{Lambert Graben}}, {{East Antarctica}}: {{A}} Precursor for {{Cenozoic}} Relief Development in an Intraplate and Glaciated Region},
  shorttitle = {Brittle Deformation Relating to the {{Carboniferous}}\textendash{{Cretaceous}} Evolution of the {{Lambert Graben}}, {{East Antarctica}}},
