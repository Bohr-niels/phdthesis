@article{mutosubglacial2013,
  title = {Subglacial Bathymetry and Sediment Layer Distribution beneath the {{Pine Island Glacier}} Ice Shelf, {{West Antarctica}}, Modeled Using Aerogravity and Autonomous Underwater Vehicle Data},
  author = {Muto, Atsuhiro and Anandakrishnan, Sridhar and Alley, Richard B.},
