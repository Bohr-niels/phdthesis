@article{rignotfour2019,
  title = {Four Decades of {{Antarctic Ice Sheet}} Mass Balance from 1979\textendash 2017},
  author = {Rignot, Eric and Mouginot, J{\'e}r{\'e}mie and Scheuchl, Bernd and {van den Broeke}, Michiel and {van Wessem}, Melchior J. and Morlighem, Mathieu},
  year = {2019},
  month = jan,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {116},
  number = {4},
  pages = {1095--1103},
  publisher = {{Proceedings of the National Academy of Sciences}},
  doi = {10.1073/pnas.1812883116},
  urldate = {2022-11-30},
  abstract = {We use updated drainage inventory, ice thickness, and ice velocity data to calculate the grounding line ice discharge of 176 basins draining the Antarctic Ice Sheet from 1979 to 2017. We compare the results with a surface mass balance model to deduce the ice sheet mass balance. The total mass loss increased from 40  {$\pm$} {$\pm$}  9 Gt/y in 1979\textendash 1990 to 50  {$\pm$} {$\pm$}  14 Gt/y in 1989\textendash 2000, 166  {$\pm$} {$\pm$}  18 Gt/y in 1999\textendash 2009, and 252  {$\pm$} {$\pm$}  26 Gt/y in 2009\textendash 2017. In 2009\textendash 2017, the mass loss was dominated by the Amundsen/Bellingshausen Sea sectors, in West Antarctica (159  {$\pm$} {$\pm$}  8 Gt/y), Wilkes Land, in East Antarctica (51  {$\pm$} {$\pm$}  13 Gt/y), and West and Northeast Peninsula (42  {$\pm$} {$\pm$}  5 Gt/y). The contribution to sea-level rise from Antarctica averaged 3.6  {$\pm$} {$\pm$}  0.5 mm per decade with a cumulative 14.0  {$\pm$} {$\pm$}  2.0 mm since 1979, including 6.9  {$\pm$} {$\pm$}  0.6 mm from West Antarctica, 4.4  {$\pm$} {$\pm$}  0.9 mm from East Antarctica, and 2.5  {$\pm$} {$\pm$}  0.4 mm from the Peninsula (i.e., East Antarctica is a major participant in the mass loss). During the entire period, the mass loss concentrated in areas closest to warm, salty, subsurface, circumpolar deep water (CDW), that is, consistent with enhanced polar westerlies pushing CDW toward Antarctica to melt its floating ice shelves, destabilize the glaciers, and raise sea level.},
