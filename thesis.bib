@article{constantinocook2023,
  title = {Cook {{Ice Shelf}} and {{Ninnis Glacier Tongue Bathymetry From Inversion}} of {{Operation Ice Bridge Airborne Gravity Data}}},
  author = {Constantino, Renata R. and Tinto, Kirsty J.},
