@article{paxmanreconstructions2019,
  title = {Reconstructions of {{Antarctic}} Topography since the {{Eocene}}\textendash{{Oligocene}} Boundary},
  author = {Paxman, Guy J. G. and Jamieson, Stewart S.R. and Hochmuth, Katharina and Gohl, Karsten and Bentley, Michael J. and Leitchenkov, German and Ferraccioli, Fausto},
