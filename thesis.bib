}

@article{yokoyamawidespread2016,
  title = {Widespread Collapse of the {{Ross Ice Shelf}} during the Late {{Holocene}}},
  author = {Yokoyama, Yusuke and Anderson, John B. and Yamane, Masako and Simkins, Lauren M. and Miyairi, Yosuke and Yamazaki, Takahiro and Koizumi, Mamito and Suga, Hisami and Kusahara, Kazuya and Prothro, Lindsay and Hasumi, Hiroyasu and Southon, John R. and Ohkouchi, Naohiko},
