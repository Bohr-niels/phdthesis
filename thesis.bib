@article{schroedermultidecadal2019,
  title = {Multidecadal Observations of the {{Antarctic}} Ice Sheet from Restored Analog Radar Records},
  author = {Schroeder, Dustin M. and Dowdeswell, Julian A. and Siegert, Martin J. and Bingham, Robert G. and Chu, Winnie and MacKie, Emma J. and Siegfried, Matthew R. and Vega, Katherine I. and Emmons, John R. and Winstein, Keith},
