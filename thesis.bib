@article{readingantarctic2022,
  title = {Antarctic Geothermal Heat Flow and Its Implications for Tectonics and Ice Sheets},
  author = {Reading, Anya M. and St{\aa}l, Tobias and Halpin, Jacqueline A. and L{\"o}sing, Mareen and Ebbing, J{\"o}rg and Shen, Weisen and McCormack, Felicity S. and Siddoway, Christine S. and Hasterok, Derrick},
