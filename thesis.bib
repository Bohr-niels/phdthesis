@article{vajdagravity2008,
  title = {Gravity Disturbances in Regions of Negative Heights: {{A}} Reference Quasi-Ellipsoid Approach},
  shorttitle = {Gravity Disturbances in Regions of Negative Heights},
  author = {Vajda, P. and Ellmann, A. and Meurers, B. and Van{\'i}{\v c}ek, P. and Nov{\'a}k, P. and Tenzer, R.},
  year = {2008},
