@article{anbathymetry2019,
  title = {Bathymetry of {{Southeast Greenland From Oceans Melting Greenland}} ({{OMG}}) {{Data}}},
  author = {An, Lu and Rignot, Eric and Chauche, Nolwenn and Holland, David M. and Holland, Denise and Jakobsson, Martin and Kane, Emily and Wood, Michael and Klaucke, Ingo and Morlighem, Mathieu and Velicogna, Isabella and Weinrebe, Wilhelm and Willis, Josh K.},
  year = {2019},
