  issn = {1866-3516},
  doi = {10.5194/essd-12-887-2020},
  urldate = {2023-05-15},
  abstract = {In recent decades, rapid ice shelf disintegration along the Antarctic Peninsula has had a global impact through enhancing outlet glacier flow and hence sea level rise and the freshening of Antarctic Bottom Water. Ice shelf thinning due to basal melting results from the circulation of relatively warm water in the underlying ocean cavity. However, the effect of sub-shelf circulation on future ice shelf stability cannot be predicted accurately with computer simulations if the geometry of the ice shelf cavity is unknown. To address this deficit for Larsen C Ice Shelf, West Antarctica, we integrate new water column thickness measurements from recent seismic campaigns with existing observations. We present these new data here along with an updated bathymetry grid of the ocean cavity. Key findings include a relatively deep seabed to the southeast of the Kenyon Peninsula, along the grounding line and around the key ice shelf pinning-point of Bawden Ice Rise. In addition, we can confirm that the cavity's southern trough stretches from Mobiloil Inlet to the open ocean. These areas of deep seabed will influence ocean circulation and tidal mixing and will therefore affect the basal-melt distribution. These results will help constrain models of ice shelf cavity circulation with the aim of improving our understanding of sub-shelf processes and their potential influence on ice shelf stability.},
  langid = {english},
  file = {/home/matt/Zotero/storage/CDRVGPYL/Brisbourne et al_2020_An updated seabed bathymetry beneath Larsen C Ice Shelf, Antarctic Peninsula.pdf;/home/matt/Zotero/storage/LNB8ZYFH/Brisbourne et al_2020_An updated seabed bathymetry beneath Larsen C Ice Shelf, Antarctic Peninsula.pdf}
}

@article{burton-johnsongeothermal2020,
  title = {Geothermal Heat Flow in {{Antarctica}}: Current and Future Directions},
  shorttitle = {Geothermal Heat Flow in {{Antarctica}}},
  author = {{Burton-Johnson}, Alex and Dziadek, Ricarda and Martin, Carlos},
  year = {2020},
  journal = {The Cryosphere Discussions},
  pages = {1--45},
  doi = {10.5194/tc-2020-59},
  urldate = {2020-03-22},
  abstract = {Antarctic geothermal heat flow (GHF) affects the temperature of the ice sheet, determining its ability to slide and internally deform, as well as the behaviour of the continental crust. However, GHF remains poorly constrained, with few and sparse local, borehole-derived estimates, and large discrepancies in the magnitude and distribution of existing continent-scale estimates from geophysical models. We review the methods to extract GHF, compile borehole and probe-derived estimates from measured temperature profiles, and recommend the following future directions: 1) Obtain more borehole-derived estimates from the subglacial bedrock and englacial temperature profiles. 2) Estimate GHF beneath the interior of the East Antarctic Ice Sheet (the region most sensitive to GHF variation) via long-wavelength microwave emissivity. 3) Estimate GHF from inverse glaciological modelling, constrained by evidence for basal melting. 4) Revise geophysically-derived GHF estimates using a combination of Curie depth, seismic, and thermal isostasy models. 5) Integrate in these geophysical approaches a more accurate model of the structure and distribution of heat production elements within the crust, and considering heterogeneities in the underlying mantle. And 6) continue international interdisciplinary communication and data access.\&lt;/p\&gt;},
  langid = {english},
  file = {/home/matt/Zotero/storage/FQXMMMJR/Burton-Johnson et al_2020_Geothermal heat flow in Antarctica.pdf;/home/matt/Zotero/storage/MKR9BW3J/Burton-Johnson et al_2020_Geothermal heat flow in Antarctica.pdf}
}

